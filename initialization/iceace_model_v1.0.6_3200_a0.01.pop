rentState>GovernmentLoop</currentState>\n                    <nextState>GovernmentCredit</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>centralbank</name>\n            <description>A simple central bank agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>day_of_month_to_act</name>\n                    <description>Day of the month to act.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>unemployment_rate</name>\n                    <description>Unemployment rate. It is updated monthly.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>inflation_rate</name>\n                    <description>inflation rate. It is updated quarterly.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>consumption_goods_prices[13]</name>\n                    <description>Average goods prices. It is updated monthly</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>housing_prices[13]</name>\n                    <description>Average housing price. It is updated monthly</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>idle</name>\n                    <description>Agent initilialization.</description>\n                    <currentState>start</currentState>\n                    <nextState>CentralBankInitBalanceSheet</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>EQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Proceeding with regular agent loop.</description>\n                    <currentState>start</currentState>\n                    <nextState>CentralBankLoop</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>NEQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>centralbank_init_balancesheet</name>\n                    <description>Synchronizing the bank debts.</description>\n                    <currentState>CentralBankInitBalanceSheet</currentState>\n                    <nextState>CentralBankLoop</nextState>\n                    <inputs>\n                        <input>\n                            <messageName>bank_centralbank_init_debt</messageName>\n                        </input>\n                        <input>\n                            <messageName>bank_centralbank_init_deposit</messageName>\n                        </input>\n                        <input>\n                            <messageName>fund_centralbank_init_deposit</messageName>\n                        </input>\n                        <input>\n                            <messageName>gov_centralbank_init_deposit</messageName>\n                        </input>\n                    </inputs>\n                </function>\n                <function>\n                    <name>centralbank_iterate</name>\n                    <description>Resumes its regular iteration.</description>\n                    <currentState>CentralBankLoop</currentState>\n                    <nextState>CentralBankCredit</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>jpoffice</name>\n            <description>Job placement office.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>idle</name>\n                    <description>Agent initilialization.</description>\n                    <currentState>start</currentState>\n                    <nextState>JPOfficeInitEmployment</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>EQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Proceeding with regular agent loop.</description>\n                    <currentState>start</currentState>\n                    <nextState>JPOfficeLoop</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>NEQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>jpoffice_init_employment</name>\n                    <description>Job placement office collects household and firm ids and forms network of initial employment.</description>\n                    <currentState>JPOfficeInitEmployment</currentState>\n                    <nextState>JPOfficeLoop</nextState>\n                    <inputs>\n                        <input>\n                            <messageName>household_jpoffice_id</messageName>\n                            <sort>\n                                <key>household_id</key>\n                                <order>ascend</order>\n                            </sort>\n                        </input>\n                        <input>\n                            <messageName>firm_jpoffice_id</messageName>\n                            <sort>\n                                <key>firm_id</key>\n                                <order>ascend</order>\n                            </sort>\n                        </input>\n                    </inputs>\n                    <outputs>\n                        <output>\n                            <messageName>jpoffice_household_employer</messageName>\n                        </output>\n                        <output>\n                            <messageName>jpoffice_firm_employee</messageName>\n                        </output>\n                    </outputs>\n                </function>\n                <function>\n                    <name>jpoffice_iterate</name>\n                    <description>Notifies its id.</description>\n                    <currentState>JPOfficeLoop</currentState>\n                    <nextState>JPOLabourMarket</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>mall</name>\n            <description>Shopping mall.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>mall_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>MallConsumptionMarket</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>reagency</name>\n            <description>Real Estate Agency.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>day_of_month_to_act</name>\n                    <description>Activation day.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>reagency_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>REAgencyHousingMarket</nextState>\n                </function>\n            </functions>\n        </xagent>\n    </agents>\n    <messages>\n        <message>\n            <name>firm_bank_init_loans</name>\n            <description>Firm sends out its initialized loan info to bank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The bank from which initial loan has gotten. </description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of loan.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>firm_bank_init_deposit</name>\n            <description>Firm sends out its liquid money to bank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The bank where liquid money to be deposited.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>firm_bank_update_deposit</name>\n            <description>Firm sends out its liquid money to bank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The bank where liquid money to be deposited.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>household_bank_init_mortgages</name>\n            <description>Household sends out its initialized amount of mortgage info to its bank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The bank from which initial mortgage has been acquired.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of mortgage.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>household_bank_init_deposit</name>\n            <description>Household sends out its liquid money to bank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The bank where liquid money to be deposited.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>household_bank_update_deposit</name>\n            <description>Household sends out its liquid money to bank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The bank where liquid money to be deposited.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>bank_centralbank_init_debt</name>\n            <description>Bank informs its centralbank debt for initilialization.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The deptor bank id.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of debt.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>bank_centralbank_init_deposit</name>\n            <description>Bank sends out its liquid money to centralbank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The depositor bank id.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>bank_centralbank_update_deposit</name>\n            <description>Bank sends out its liquid money to centralbank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The depositor bank id.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        \n        <message>\n            <name>fund_centralbank_init_deposit</name>\n            <description>Fund sends out its liquid money to centralbank.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>fund_centralbank_update_deposit</name>\n            <description>Fund sends out its liquid money to centralbank.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        \n        <message>\n            <name>gov_centralbank_init_deposit</name>\n            <description>Government sends out its liquid money to centralbank.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>gov_centralbank_update_deposit</name>\n            <description>Government sends out its liquid money to centralbank.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>interest_rate</name>\n            <description>The interest rate sent by the central bank.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>rate</name>\n                    <description>The rate.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>capital_tax_rate</name>\n            <description>Government tax rate on capital income.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>value</name>\n                    <description>Rate of the tax.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>labour_tax_rate</name>\n            <description>Government tax rate on labour income.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>value</name>\n                    <description>Rate of the tax.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>household_jpoffice_id</name>\n            <description>Household notifies his/her id.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>household_id</name>\n                    <description>The agent id.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>jpoffice_household_employer</name>\n            <description>Firm notifies its id.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>household_id</name>\n                    <description>The agent id.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>employer_id</name>\n                    <description>The agent id.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>position</name>\n                    <description>The position. 1 for managers 0 for regulars.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>firm_jpoffice_id</name>\n            <description>Firm notifies its id.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>firm_id</name>\n                    <description>The agent id.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>isconstructor</name>\n                    <description>Firm type.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>jpoffice_firm_employee</name>\n            <description>Firm notifies its id.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>firm_id</name>\n                    <description>The agent id.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>employee_id</name>\n                    <description>The agent id.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>position</name>\n                    <description>The position. 1 for managers 0 for regulars.</description>\n                </variable>\n            </variables>\n        </message>\n    </messages>\n</xmodel>'
