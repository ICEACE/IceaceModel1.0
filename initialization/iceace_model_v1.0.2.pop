S'<xmodel version="2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"\n    xsi:noNamespaceSchemaLocation=\'http://flame.ac.uk/schema/xmml_v2.xsd\'>\n    <!-- Model descriptions  -->\n    <name>ICEACE</name>\n    <version>Version 1.0.0, 06/03/2014</version>\n    <description>\'ICEACE - Labour Market, Production Market, Consumption Market, Credit Market and Housing Market with different mortgage types</description>\n    <models>\n        <model>\n            <file>Market_Consumption/model_consumption.xml</file>\n            <enabled>true</enabled>\n        </model>\n        <model>\n            <file>Market_Labour/model_labour.xml</file>\n            <enabled>true</enabled>\n        </model>\n        <model>\n            <file>Market_Production/model_production.xml</file>\n            <enabled>true</enabled>\n        </model>\n        <model>\n            <file>Market_Credit/model_credit.xml</file>\n            <enabled>true</enabled>\n        </model>\n        <model>\n            <file>Market_Housing/model_housing.xml</file>\n            <enabled>true</enabled>\n        </model>\n    </models>\n    <environment>\n        <constants>\n            <variable>\n                <type>int</type>\n                <name>resume_mode</name>\n                <description>Flag, 0 or 1.  To re-start a simulation from a snapshot , it needs to be set to 1.</description>\n            </variable>\n            <variable>\n                <type>int</type>\n                <name>print_debug_mode</name>\n                <description>Flag, 0 or 1.  To record simulation process stages and selected states for debugging purposes, it needs to be set to 1.</description>\n            </variable>\n            <variable>\n                <type>int</type>\n                <name>warning_mode</name>\n                <description>Flag, 0 or 1.  To record unexpected states, this needs to be set to 1.</description>\n            </variable>\n            <variable>\n                <type>int</type>\n                <name>data_collection_mode</name>\n                <description>Flag, 0 or 1. To record simulation data, it needs to be set to 1.</description>\n            </variable>\n            <variable>\n                <type>int</type>\n                <name>collect_household_data</name>\n                <description>Flag, 0 or 1. To record household simulation data, it needs to be set to 1.</description>\n            </variable>\n            <variable>\n                <type>int</type>\n                <name>collect_firm_data</name>\n                <description>Flag, 0 or 1. To record firm simulation data, it needs to be set to 1.</description>\n            </variable>\n        </constants>\n        <functionFiles>\n        <!-- List of C files that hold agent functions -->\n            <file>library_functions.c</file>\n            <file>firm_functions_top.c</file>\n            <file>household_functions_top.c</file>\n            <file>bank_functions_top.c</file>\n            <file>government_functions_top.c</file>\n            <file>centralbank_functions_top.c</file>\n            <file>equityfund_functions_top.c</file>\n            <file>jpoffice_functions_top.c</file>\n            <file>mall_functions_top.c</file>\n            <file>reagency_functions_top.c</file>\n        </functionFiles>\n        <dataTypes>\n            <dataType>\n                <name>transaction</name>\n                <description>A generic transaction data structure.</description>\n                <variables>\n                    <variable>\n                        <type>int</type>\n                        <name>quantity</name>\n                        <description>Number of items within a period.</description>\n                    </variable>\n                    <variable>\n                        <type>double</type>\n                        <name>avg_price</name>\n                        <description>Average price on overall transactions.</description>\n                    </variable>\n                </variables>\n            </dataType>\n        </dataTypes>\n        <timeUnits>\n            <timeUnit>\n                <name>daily</name>\n                <unit>iteration</unit>\n                <period>1</period>\n            </timeUnit>\n            <timeUnit>\n                <name>weekly</name>\n                <unit>daily</unit>\n                <period>5</period>\n            </timeUnit>\n            <timeUnit>\n                <name>monthly</name>\n                <unit>weekly</unit>\n                <period>4</period>\n            </timeUnit>\n            <timeUnit>\n                <name>quarterly</name>\n                <unit>monthly</unit>\n                <period>3</period>\n            </timeUnit>\n            <timeUnit>\n                <name>yearly</name>\n                <unit>monthly</unit>\n                <period>12</period>\n            </timeUnit>\n        </timeUnits>\n    </environment>\n\n\t<!-- List of agents -->\n    <agents>\n        <xagent>\n            <name>firm</name>\n            <description>Firm agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>Primary bank of a firm.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>isconstructor</name>\n                    <description>Identifies if the firm is a constractor firm or not.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>day_of_month_to_act</name>\n                    <description>The day of the month to act at production and labour.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>isinsolvent</name>\n                    <description>The variable indicates if the firm is a replacement of an insolvent producer.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>idle</name>\n                    <description>Agent initilialization.</description>\n                    <currentState>start</currentState>\n                    <nextState>FirmInitPostId</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>EQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Proceeding with regular agent loop.</description>\n                    <currentState>start</currentState>\n                    <nextState>FirmLoop</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>NEQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>firm_init_post_id</name>\n                    <description>Firm posts its Id for employment initilialization.</description>\n                    <currentState>FirmInitPostId</currentState>\n                    <nextState>FirmInitEmployment</nextState>\n                    <outputs>\n                        <output>\n                            <messageName>firm_jpoffice_id</messageName>\n                        </output>\n                    </outputs>\n                </function>\n                <function>\n                    <name>firm_init_employment</name>\n                    <description>Firm recieves its list of employees.</description>\n                    <currentState>FirmInitEmployment</currentState>\n                    <nextState>FirmInitBalanceSheet</nextState>\n                    <inputs>\n                        <input>\n                            <messageName>jpoffice_firm_employee</messageName>\n                            <filter>\n                                <!-- checks self id. -->\n                                <lhs><value>a.id</value></lhs>\n                                <op>EQ</op>\n                                <rhs><value>m.firm_id</value></rhs>\n                            </filter>\n                        </input>\n                    </inputs>\n                </function>\n                <function>\n                    <name>firm_init_balancesheet</name>\n                    <description>Synchronizing the mortgage debt and deposit with respective banks.</description>\n                    <currentState>FirmInitBalanceSheet</currentState>\n                    <nextState>FirmLoop</nextState>\n                    <outputs>\n                        <output>\n                            <messageName>firm_bank_init_loans</messageName>\n                        </output>\n                        <output>\n                            <messageName>firm_bank_init_deposit</messageName>\n                        </output>\n                    </outputs>\n                </function>\n                <function>\n                    <name>firm_iterate</name>\n                    <description>Resumes with its regular iteration loop.</description>\n                    <currentState>FirmLoop</currentState>\n                    <nextState>FirmCreditStart</nextState>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Skip the bank account updates.</description>\n                    <currentState>FirmUpdateDeposits</currentState>\n                    <nextState>FirmCreditPolicyChecks</nextState>\n                    <condition>\n                        <not><time>\n                            <period>monthly</period>\n                            <phase>0</phase>\n                        </time></not>\n                    </condition>\n                </function>\n                <function>\n                    <name>firm_update_bank_account</name>\n                    <description>Deposits/updates its liquidity to bank.</description>\n                    <currentState>FirmUpdateDeposits</currentState>\n                    <nextState>FirmCreditPolicyChecks</nextState>\n                    <condition>\n                        <time>\n                            <period>monthly</period>\n                            <phase>0</phase>\n                        </time>\n                    </condition>\n                    <outputs>\n                        <output>\n                            <messageName>firm_bank_update_deposit</messageName>\n                        </output>\n                    </outputs>\n                </function>\n             </functions>\n        </xagent>\n        <xagent>\n            <name>household</name>\n            <description>A consumer and labourer agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The ID of the bank an household has. Household applies to her bank for mortgage and deposits.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>idle</name>\n                    <description>Agent initilialization.</description>\n                    <currentState>start</currentState>\n                    <nextState>HouseholdInitPostId</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>EQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Proceeding with regular agent loop.</description>\n                    <currentState>start</currentState>\n                    <nextState>HouseholdLoop</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>NEQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>household_init_post_id</name>\n                    <description>Household posts its Id for employment initilialization.</description>\n                    <currentState>HouseholdInitPostId</currentState>\n                    <nextState>HouseholdInitEmployment</nextState>\n                    <outputs>\n                        <output>\n                            <messageName>household_jpoffice_id</messageName>\n                        </output>\n                    </outputs>\n                </function>\n                <function>\n                    <name>household_init_employment</name>\n                    <description>Household recieves agent Id of his employer.</description>\n                    <currentState>HouseholdInitEmployment</currentState>\n                    <nextState>HouseholdInitBalanceSheet</nextState>\n                    <inputs>\n                        <input>\n                            <messageName>jpoffice_household_employer</messageName>\n                            <filter>\n                                <!-- checks self id. -->\n                                <lhs><value>a.id</value></lhs>\n                                <op>EQ</op>\n                                <rhs><value>m.household_id</value></rhs>\n                            </filter>\n                        </input>\n                    </inputs>\n                </function>\n                <function>\n                    <name>household_init_balancesheet</name>\n                    <description>Synchronizing the mortgage debt and deposit with respective banks.</description>\n                    <currentState>HouseholdInitBalanceSheet</currentState>\n                    <nextState>HouseholdLoop</nextState>\n                    <outputs>\n                        <output>\n                            <messageName>household_bank_init_mortgages</messageName>\n                        </output>\n                        <output>\n                            <messageName>household_bank_init_deposit</messageName>\n                        </output>\n                    </outputs>\n                </function>\n                <function>\n                    <name>household_iterate</name>\n                    <description>Resumes its regular iteration.</description>\n                    <currentState>HouseholdLoop</currentState>\n                    <nextState>HHCreditStart</nextState>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Skip the bank account updates.</description>\n                    <currentState>HouseholdUpdateDeposits</currentState>\n                    <nextState>HHCreditPolicyChecks</nextState>\n                    <condition>\n                        <not><time>\n                            <period>monthly</period>\n                            <phase>0</phase>\n                        </time></not>\n                    </condition>\n                </function>\n                <function>\n                    <name>household_update_bank_account</name>\n                    <description>Deposits/updates its liquidity to bank.</description>\n                    <currentState>HouseholdUpdateDeposits</currentState>\n                    <nextState>HHCreditPolicyChecks</nextState>\n                    <condition>\n                        <time>\n                            <period>monthly</period>\n                            <phase>0</phase>\n                        </time>\n                    </condition>\n                    <outputs>\n                        <output>\n                            <messageName>household_bank_update_deposit</messageName>\n                        </output>\n                    </outputs>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>equityfund</name>\n            <description>Equity Fund agency.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>equityfund_init</name>\n                    <description>Agent initilialization.</description>\n                    <currentState>start</currentState>\n                    <nextState>EquityFundLoop</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>EQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                    <outputs>\n                        <output>\n                            <messageName>fund_centralbank_init_deposit</messageName>\n                        </output>\n                    </outputs>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Proceeding with regular agent loop.</description>\n                    <currentState>start</currentState>\n                    <nextState>EquityFundLoop</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>NEQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>equityfund_iterate</name>\n                    <description>Notifies its id.</description>\n                    <currentState>EquityFundLoop</currentState>\n                    <nextState>EquityFundCredit</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>bank</name>\n            <description>A simple bank agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>day_of_month_to_act</name>\n                    <description>Activation day.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>day_of_week_to_act</name>\n                    <description>Day of the week to act.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>idle</name>\n                    <description>Agent initilialization.</description>\n                    <currentState>start</currentState>\n                    <nextState>BankInitLoans</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>EQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Proceeding with regular agent loop.</description>\n                    <currentState>start</currentState>\n                    <nextState>BankLoop</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>NEQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>bank_init_loans</name>\n                    <description>Gathers loans from firms.</description>\n                    <currentState>BankInitLoans</currentState>\n                    <nextState>BankInitMortgages</nextState>\n                    <inputs>\n                        <input>\n                            <messageName>firm_bank_init_loans</messageName>\n                            <filter>\n                                <!-- checks self id. -->\n                                <lhs><value>a.id</value></lhs>\n                                <op>EQ</op>\n                                <rhs><value>m.bank_id</value></rhs>\n                            </filter>\n                        </input>\n                    </inputs>\n                </function>\n                <function>\n                    <name>bank_init_mortgages</name>\n                    <description>Gather mortgages from households.</description>\n                    <currentState>BankInitMortgages</currentState>\n                    <nextState>BankInitDeposits</nextState>\n                    <inputs>\n                        <input>\n                            <messageName>household_bank_init_mortgages</messageName>\n                            <filter>\n                                <!-- checks self id. -->\n                                <lhs><value>a.id</value></lhs>\n                                <op>EQ</op>\n                                <rhs><value>m.bank_id</value></rhs>\n                            </filter>\n                        </input>\n                    </inputs>\n                </function>\n                <function>\n                    <name>bank_init_deposits</name>\n                    <description>Gather deposits from firms and households.</description>\n                    <currentState>BankInitDeposits</currentState>\n                    <nextState>BankInitBalanceSheet</nextState>\n                    <inputs>\n                        <input>\n                            <messageName>household_bank_init_deposit</messageName>\n                            <filter>\n                                <!-- checks self id. -->\n                                <lhs><value>a.id</value></lhs>\n                                <op>EQ</op>\n                                <rhs><value>m.bank_id</value></rhs>\n                            </filter>\n                        </input>\n                        <input>\n                            <messageName>firm_bank_init_deposit</messageName>\n                            <filter>\n                                <!-- checks self id. -->\n                                <lhs><value>a.id</value></lhs>\n                                <op>EQ</op>\n                                <rhs><value>m.bank_id</value></rhs>\n                            </filter>\n                        </input>\n                    </inputs>\n                </function>\n                <function>\n                    <name>bank_init_balancesheet</name>\n                    <description>Bank initializes total assets, equity, debt to central bank and liquidity.</description>\n                    <currentState>BankInitBalanceSheet</currentState>\n                    <nextState>BankLoop</nextState>\n                    <outputs>\n                        <output>\n                            <messageName>bank_centralbank_init_debt</messageName>\n                        </output>\n                        <output>\n                            <messageName>bank_centralbank_init_deposit</messageName>\n                        </output>\n                    </outputs>\n                </function>\n                <function>\n                    <name>bank_iterate</name>\n                    <description>Bank resumes its iterative functions.</description>\n                    <currentState>BankLoop</currentState>\n                    <nextState>BankCreditStart</nextState>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Skip the bank account updates.</description>\n                    <currentState>BankUpdateDeposits</currentState>\n                    <nextState>BankCreditPolicyChecks</nextState>\n                    <condition>\n                        <not><time>\n                            <period>monthly</period>\n                            <phase>0</phase>\n                        </time></not>\n                    </condition>\n                </function>\n                <function>\n                    <name>bank_update_deposits</name>\n                    <description>Deposits/updates its liquidity to bank.</description>\n                    <currentState>BankUpdateDeposits</currentState>\n                    <nextState>BankCreditPolicyChecks</nextState>\n                    <condition>\n                        <time>\n                            <period>monthly</period>\n                            <phase>0</phase>\n                        </time>\n                    </condition>\n                    <inputs>\n                        <input>\n                            <messageName>household_bank_update_deposit</messageName>\n                            <filter>\n                                <!-- checks self id. -->\n                                <lhs><value>a.id</value></lhs>\n                                <op>EQ</op>\n                                <rhs><value>m.bank_id</value></rhs>\n                            </filter>\n                        </input>\n                        <input>\n                            <messageName>firm_bank_update_deposit</messageName>\n                            <filter>\n                                <!-- checks self id. -->\n                                <lhs><value>a.id</value></lhs>\n                                <op>EQ</op>\n                                <rhs><value>m.bank_id</value></rhs>\n                            </filter>\n                        </input>\n                    </inputs>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>government</name>\n            <description>A simple government agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>government_init</name>\n                    <description>Agent runtime initilialization.</description>\n                    <currentState>start</currentState>\n                    <nextState>GovernmentLoop</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>EQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                    <outputs>\n                        <output>\n                            <messageName>gov_centralbank_init_deposit</messageName>\n                        </output>\n                    </outputs>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Proceeding with regular agent loop.</description>\n                    <currentState>start</currentState>\n                    <nextState>GovernmentLoop</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>NEQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>government_iterate</name>\n                    <description>Regular loop start point.</description>\n                    <currentState>GovernmentLoop</currentState>\n                    <nextState>GovernmentCredit</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>centralbank</name>\n            <description>A simple central bank agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>day_of_month_to_act</name>\n                    <description>Day of the month to act.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>unemployment_rate</name>\n                    <description>Unemployment rate. It is updated monthly.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>inflation_rate</name>\n                    <description>inflation rate. It is updated quarterly.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>consumption_goods_prices[12]</name>\n                    <description>Unemployment rate. It is updated monthly</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>idle</name>\n                    <description>Agent initilialization.</description>\n                    <currentState>start</currentState>\n                    <nextState>CentralBankInitBalanceSheet</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>EQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Proceeding with regular agent loop.</description>\n                    <currentState>start</currentState>\n                    <nextState>CentralBankLoop</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>NEQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>centralbank_init_balancesheet</name>\n                    <description>Synchronizing the bank debts.</description>\n                    <currentState>CentralBankInitBalanceSheet</currentState>\n                    <nextState>CentralBankLoop</nextState>\n                    <inputs>\n                        <input>\n                            <messageName>bank_centralbank_init_debt</messageName>\n                        </input>\n                        <input>\n                            <messageName>bank_centralbank_init_deposit</messageName>\n                        </input>\n                        <input>\n                            <messageName>fund_centralbank_init_deposit</messageName>\n                        </input>\n                        <input>\n                            <messageName>gov_centralbank_init_deposit</messageName>\n                        </input>\n                    </inputs>\n                </function>\n                <function>\n                    <name>centralbank_iterate</name>\n                    <description>Resumes its regular iteration.</description>\n                    <currentState>CentralBankLoop</currentState>\n                    <nextState>CentralBankCredit</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>jpoffice</name>\n            <description>Job placement office.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>idle</name>\n                    <description>Agent initilialization.</description>\n                    <currentState>start</currentState>\n                    <nextState>JPOfficeInitEmployment</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>EQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Proceeding with regular agent loop.</description>\n                    <currentState>start</currentState>\n                    <nextState>JPOfficeLoop</nextState>\n                    <condition>\n                        <lhs><value>a.it_no</value></lhs>\n                        <op>NEQ</op>\n                        <rhs><value>0</value></rhs>\n                    </condition>\n                </function>\n                <function>\n                    <name>jpoffice_init_employment</name>\n                    <description>Job placement office collects household and firm ids and forms network of initial employment.</description>\n                    <currentState>JPOfficeInitEmployment</currentState>\n                    <nextState>JPOfficeLoop</nextState>\n                    <inputs>\n                        <input>\n                            <messageName>household_jpoffice_id</messageName>\n                            <sort>\n                                <key>household_id</key>\n                                <order>ascend</order>\n                            </sort>\n                        </input>\n                        <input>\n                            <messageName>firm_jpoffice_id</messageName>\n                            <sort>\n                                <key>firm_id</key>\n                                <order>ascend</order>\n                            </sort>\n                        </input>\n                    </inputs>\n                    <outputs>\n                        <output>\n                            <messageName>jpoffice_household_employer</messageName>\n                        </output>\n                        <output>\n                            <messageName>jpoffice_firm_employee</messageName>\n                        </output>\n                    </outputs>\n                </function>\n                <function>\n                    <name>jpoffice_iterate</name>\n                    <description>Notifies its id.</description>\n                    <currentState>JPOfficeLoop</currentState>\n                    <nextState>JPOLabourMarket</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>mall</name>\n            <description>Shopping mall.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>mall_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>MallConsumptionMarket</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>reagency</name>\n            <description>Real Estate Agency.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>day_of_month_to_act</name>\n                    <description>Activation day.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>reagency_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>REAgencyHousingMarket</nextState>\n                </function>\n            </functions>\n        </xagent>\n    </agents>\n    <messages>\n        <message>\n            <name>firm_bank_init_loans</name>\n            <description>Firm sends out its initialized loan info to bank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The bank from which initial loan has gotten. </description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of loan.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>firm_bank_init_deposit</name>\n            <description>Firm sends out its liquid money to bank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The bank where liquid money to be deposited.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>firm_bank_update_deposit</name>\n            <description>Firm sends out its liquid money to bank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The bank where liquid money to be deposited.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>household_bank_init_mortgages</name>\n            <description>Household sends out its initialized amount of mortgage info to its bank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The bank from which initial mortgage has been acquired.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of mortgage.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>household_bank_init_deposit</name>\n            <description>Household sends out its liquid money to bank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The bank where liquid money to be deposited.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>household_bank_update_deposit</name>\n            <description>Household sends out its liquid money to bank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The bank where liquid money to be deposited.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>bank_centralbank_init_debt</name>\n            <description>Bank informs its centralbank debt for initilialization.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The deptor bank id.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of debt.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>bank_centralbank_init_deposit</name>\n            <description>Bank sends out its liquid money to centralbank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The depositor bank id.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>bank_centralbank_update_deposit</name>\n            <description>Bank sends out its liquid money to centralbank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The depositor bank id.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        \n        <message>\n            <name>fund_centralbank_init_deposit</name>\n            <description>Fund sends out its liquid money to centralbank.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>fund_centralbank_update_deposit</name>\n            <description>Fund sends out its liquid money to centralbank.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        \n        <message>\n            <name>gov_centralbank_init_deposit</name>\n            <description>Government sends out its liquid money to centralbank.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>gov_centralbank_update_deposit</name>\n            <description>Government sends out its liquid money to centralbank.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>interest_rate</name>\n            <description>The interest rate sent by the central bank.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>rate</name>\n                    <description>The rate.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>capital_tax_rate</name>\n            <description>Government tax rate on capital income.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>value</name>\n                    <description>Rate of the tax.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>labour_tax_rate</name>\n            <description>Government tax rate on labour income.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>value</name>\n                    <description>Rate of the tax.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>household_jpoffice_id</name>\n            <description>Household notifies his/her id.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>household_id</name>\n                    <description>The agent id.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>jpoffice_household_employer</name>\n            <description>Firm notifies its id.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>household_id</name>\n                    <description>The agent id.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>employer_id</name>\n                    <description>The agent id.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>position</name>\n                    <description>The position. 1 for managers 0 for regulars.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>firm_jpoffice_id</name>\n            <description>Firm notifies its id.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>firm_id</name>\n                    <description>The agent id.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>isconstructor</name>\n                    <description>Firm type.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>jpoffice_firm_employee</name>\n            <description>Firm notifies its id.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>firm_id</name>\n                    <description>The agent id.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>employee_id</name>\n                    <description>The agent id.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>position</name>\n                    <description>The position. 1 for managers 0 for regulars.</description>\n                </variable>\n            </variables>\n        </message>\n    </messages>\n</xmodel>'
